use STpolicy *
use map_tools *
use embed_python *

apply_pattern package_include
private
apply_pattern ST_library option=-no_share

# simple application, 
macro source "*.cxx" WIN32 "*.cxx *.h *.py"
application pointfit -s=pointfit $(source) -s=../python $(source)



